	<name>Generate Random Password Policy</name>
	<description>Used when generating random passwords for resource accounts. Passwords will be 32 alphanumeric characters.</description>
	<stringPolicy>
		<description>String validation policy</description>
		<limitations>
			<minLength>32</minLength>
			<maxLength>32</maxLength>
			<minUniqueChars>6</minUniqueChars>
		</limitations>
	</stringPolicy>
</valuePolicy>